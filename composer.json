{
  "name": "steveolotu/featuredocu",
  "description": "Uses annotations to auto-generate documentation of business logic features in the code.",
  "type": "library",
  "keywords": [
    "symfony", "documentation", "annotations"
  ],
  "require": {
    "doctrine/annotations": "^1.11",
    "doctrine/common": "^3.1",
    "php": ">=7.3",
    "symfony/framework-bundle": "^5.0",
    "symfony/http-client": "^5.2.1",
    "symfony/intl": "^5.2.1",
    "symfony/twig-bundle": "^5.2.1",
    "symfony/twig-pack": "1.*",
    "twig/twig": "^3.2.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16.7",
    "phpstan/phpstan": "^0.12.56",
    "symfony/phpunit-bridge": "^4.4|^5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Steve Olotu",
      "email": "mail@steveolotu.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "SteveOlotu\\FeatureDoco\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SteveOlotu\\FeatureDoco\\Tests\\": "tests/"
    }
  }
}
